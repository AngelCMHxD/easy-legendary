{
	"name": "easy-legendary",
	"version": "0.1.0",
	"description": "easy-legendary is a tool that execute legendary-gl commands for you, goodbye to executing commands!",
	"main": "index.js",
	"scripts": {
		"start": "node .",
		"build": "npx pkg . & copy .\\node_modules\\create-desktop-shortcuts\\src\\windows.vbs .\\dist\\shortcut.vbs",
		"build:no-fetched": "npx pkg . --build & copy .\\node_modules\\create-desktop-shortcuts\\src\\windows.vbs .\\dist\\shortcut.vbs"
	},
	"author": "AngelCMHxD",
	"license": "MIT",
	"bin": "index.js",
	"pkg": {
		"targets": [
			"node16-win-x64"
		],
		"scripts": [
			"commands/**/*.js",
			"utils/**/*.js"
		],
		"outputPath": "dist"
	},
	"dependencies": {
		"cli-progress": "^3.11.1",
		"create-desktop-shortcuts": "^1.10.0",
		"fuzzy": "^0.1.3",
		"inquirer": "^8.2.0",
		"inquirer-autocomplete-prompt": "^1.4.0",
		"node-fetch": "^2.6.7",
		"pkg": "^5.5.1",
		"request": "^2.88.2",
		"unzipper": "^0.10.11"
	}
}
